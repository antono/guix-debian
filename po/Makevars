# Makefile variables for PO directory in any package using GNU gettext.

DOMAIN = $(PACKAGE)

subdir = po
top_builddir = ..

# These options get passed to xgettext.
XGETTEXT_OPTIONS =					\
  --language=Scheme --from-code=UTF-8			\
  --keyword=_ --keyword=N_				\
  --keyword=synopsis --keyword=description

